{
  "name": "eslint-plugin-react-query",
  "version": "0.0.0-development",
  "license": "MIT",
  "description": "ESLint rules for React Query",
  "repository": {
    "type": "git",
    "url": "https://github.com/KubaJastrz/eslint-plugin-react-query"
  },
  "homepage": "https://github.com/KubaJastrz/eslint-plugin-react-query",
  "bugs": {
    "url": "https://github.com/KubaJastrz/eslint-plugin-react-query/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "dev": "jest --watch",
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write .",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md,html,yaml,yml,svg}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "printWidth": 100,
    "singleQuote": true
  },
  "files": [
    "dist",
    "docs"
  ],
  "engines": {
    "node": ">=12"
  },
  "peerDependencies": {
    "eslint": ">=7.30"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.12.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.25",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "~4.5.5"
  },
  "publishConfig": {
    "access": "public"
  }
}

{
  "name": "eslint-plugin-react-query",
  "version": "0.0.0-development",
  "license": "MIT",
  "description": "ESLint rules for React Query",
  "repository": {
    "type": "git",
    "url": "https://github.com/KubaJastrz/eslint-plugin-react-query"
  },
  "homepage": "https://github.com/KubaJastrz/eslint-plugin-react-query",
  "bugs": {
    "url": "https://github.com/KubaJastrz/eslint-plugin-react-query/issues"
  },
  "scripts": {
    "dev": "jest --watch",
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write .",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md,html,yaml,yml,svg}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "printWidth": 100,
    "singleQuote": true
  },
  "engines": {
    "node": ">=12"
  },
  "peerDependencies": {
    "eslint": ">=7.30"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "husky": "^7.0.2",
    "jest": "^27.3.0",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.0",
    "typescript": "~4.4.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
